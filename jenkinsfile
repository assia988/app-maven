pipeline {
agent any
stages {
stage ('Compile Stage') {
steps {
withMaven(MAven : 'apache-maven-3.6.3') {
bat'mvn clean compile'
}
}
}
stage ('Testing Stage') {
steps {
withMaven(MAven : 'apache-maven-3.6.3') {
bat'mvn test'
}
}
}
stage ('package') {
steps {
withMaven(MAven : 'apache-maven-3.6.3') {
bat'mvn package'
}
}
}
}
}
